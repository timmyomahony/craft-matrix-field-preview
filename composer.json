{
    "name": "weareferal/matrix-field-preview",
    "description": "Gives you the ability to configure a preview for all your matrix field blocks, giving your clients a better publishing experience.",
    "version": "1.2.0",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "plugin",
        "neo",
        "matrix",
        "field",
        "matrix field preview"
    ],
    "support": {
        "docs": "https://github.com/weareferal/matrix-field-preview/blob/master/README.md",
        "issues": "https://github.com/weareferal/matrix-field-preview/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Timmy O'Mahony ",
            "homepage": "https://weareferal.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "weareferal\\matrixfieldpreview\\": "src/"
        }
    },
    "extra": {
        "name": "Matrix Field Preview",
        "handle": "matrix-field-preview",
        "hasCpSettings": true,
        "changelogUrl": "https://raw.githubusercontent.com/weareferal/matrix-field-preview/master/CHANGELOG.md",
        "components": {
            "blockTypeConfigService": "weareferal\\matrixfieldpreview\\services\\BlockTypeConfigService"
        },
        "class": "weareferal\\matrixfieldpreview\\MatrixFieldPreview"
    },
    "suggest": {
        "spicyweb/craft-neo": "For NEO field support"
    }
}